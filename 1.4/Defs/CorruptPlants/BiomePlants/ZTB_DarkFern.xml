    <graphicData>
      <texPath>ZTBiomes/Plant/DarkFern</texPath>
	  <drawSize>2</drawSize>
	   <shadowData> 
         <volume>(0.55, 0.17, 0.15)</volume> 
         <offset>(0,0,-0.2)</offset> 
